@{
    ViewBag.Title = "Export";
}

<form class="form-dialog" method="post" id="dataform">
    <div class="padding-dialog">
        <div class="padding-5">
            <label class="label-title">公司名称：</label>
            <input class="input-dialog" name="UserName" value="" />
        </div>
        <div class="padding-5">
            <label class="label-title">状态：</label>
            <select name="IsValidate" id="IsValidate">
                <option value="true">有效</option>
                <option value="false">无效</option>
            </select>
        </div>
        <div class="showMsg"><label id="showMsg"></label></div>
        <div class="btn-dialog">
            <input type="button" value="确定" class="btn-padding easyui-linkbutton " onclick="downLoad('/User/Export?IsValidate=' + $('#IsValidate').val(), 'post')" />
            <input type="button" value="取消" class="btn-padding easyui-linkbutton" onclick="cancel()" />
        </div>
    </div>
</form>
