@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/_Page.cshtml";
}
@model Panasia.Gemini.Web.Models.AddModel

@using AF.Core;

<form method="post" class="frmAdd">
    <div class="form-dialog" id="addForm">
    </div>
    <div class="showMsg"><label id="showMsg"></label></div>
    <div class="btn-add">
        <input type="button" value="确定" class="btn-padding easyui-linkbutton " onclick="addInfo()"/>
        <input type="button" value="取消" class="btn-padding easyui-linkbutton" onclick="cancel()" />
    </div>
</form>
    @section scripts{
        <script type="text/javascript">
            addToControl(@Html.Raw(@Model.Control.ToJson()))
        </script>
    }
